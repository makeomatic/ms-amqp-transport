{
  "node": "14.11.0",
  "auto_compose": true,
  "services": [
    "rabbitmq"
  ],
  "extras": {
    "tester": {
      "volumes": [
        "${PWD}:/src"
      ],
      "links": [
        "rabbitmq"
      ],
      "environment": {
        "NODE_ENV": "test",
        "RABBITMQ_PORT_5672_TCP_ADDR": "rabbitmq",
        "DEBUG": "proxy"
      }
    },
    "rabbitmq": {
      "ports": [
        "5672:5672",
        "15672:15672"
      ]
    }
  },
  "tests": "./test/**/*",
  "rebuild": ["microtime"]
}
