{
  "name": "@microfleet/transport-amqp",
  "description": "microservice utils based on amqp transport layer",
  "version": "0.0.0-development",
  "main": "./lib/amqp.js",
  "scripts": {
    "compile": "babel -d ./lib ./src",
    "lint": "eslint ./src",
    "test": "yarn lint && yarn test:e2e",
    "test:e2e": "mdep test run -t ./test/*.js -r microtime",
    "prepublishOnly": "yarn compile",
    "bench": "yarn compile && node ./bench/roundtrip.js",
    "semantic-release": "semantic-release",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "preparecommitmsg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $GIT_PARAMS"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 4.x.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microfleet/transport-amqp.git"
  },
  "author": "Vitaly Aminev <v@makeomatic.ca>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microfleet/transport-amqp/issues"
  },
  "homepage": "https://github.com/microfleet/transport-amqp#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@babel/plugin-transform-strict-mode": "^7.2.0",
    "@babel/register": "^7.4.4",
    "@makeomatic/deploy": "^8.4.4",
    "babel-eslint": "^10.0.2",
    "babel-plugin-istanbul": "^5.1.4",
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-makeomatic": "^3.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-mocha": "^6.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "jaeger-client": "^3.15.0",
    "microtime": "^3.0.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "stdout-stream": "^1.4.1"
  },
  "peerDependencies": {
    "common-errors": "~1.x.x",
    "stdout-stream": "~1.x.x"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "@microfleet/amqp-coffee": "^1.3.1",
    "bluebird": "^3.5.5",
    "common-errors": "^1.0.5",
    "debug": "^4.1.1",
    "eventemitter3": "^4.0.0",
    "flatstr": "^1.0.12",
    "hashlru": "^2.3.0",
    "is": "^3.3.0",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.11",
    "object-hash": "^1.3.1",
    "opentracing": "^0.14.4",
    "pino": "^5.13.0",
    "uuid": "^3.3.2"
  },
  "files": [
    "lib/",
    "src/",
    "yarn.lock"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "./node_modules/@makeomatic/deploy/git-hooks/prepare-commit-msg $HUSKY_GIT_PARAMS"
    }
  }
}
