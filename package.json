    "eventemitter3": "^2.0.3",
    "hashlru": "^2.2.0",
    "is": "^3.2.1",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.17.2",
    "object-hash": "^1.1.8",
    "uuid": "^3.1.0"
  },
  "files": [
    "lib/",
    "src/",
    "schema.json",
    "yarn.lock"
  ],
  "release": {
    "verifyConditions": []
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
